{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountName": {
      "value": "yourstorageaccount"
    },
    "resourceGroupName": {
      "value": "your-resource-group"
    },
    "acrLoginServer": {
      "value": "ghcr.io"
    },
    "containerImageName": {
      "value": "ghcr.io/YOUR_GITHUB_USERNAME/YOUR_REPO_NAME/finalvideo-worker:latest"
    },
    "blobEndpoint": {
      "value": "https://yourstorageaccount.blob.core.windows.net"
    },
    "firebaseAuthSecret": {
      "value": "your-firebase-auth-secret"
    },
    "firebaseBasePath": {
      "value": "https://your-firebase-project.firebaseio.com"
    },
    "location": {
      "value": "westeurope"
    }
  }
}

